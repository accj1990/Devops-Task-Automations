					"raw": "[\r\n  {\r\n    \"op\": \"add\",\r\n    \"path\": \"/fields/System.Title\",\r\n    \"value\": \"Teste com Descrição e Tags\"\r\n  }, \r\n  {\r\n    \"op\": \"add\",\r\n    \"path\": \"/fields/System.Description\",\r\n    \"value\": \"Descrição\"\r\n  }, \r\n  {\r\n    \"op\": \"add\",\r\n    \"path\": \"/fields/System.AssignedTo\",\r\n    \"value\": \"devvalido@organizacao.com.br\"\r\n  },\r\n  {\r\n    \"op\": \"add\",\r\n    \"path\": \"/fields/Microsoft.VSTS.Scheduling.StoryPoints\",\r\n    \"value\": \"3\"\r\n  }, \r\n  {\r\n    \"op\" : \"add\",\r\n    \"path\" :\"/fields/Microsoft.VSTS.Common.AcceptanceCriteria\",\r\n    \"value\":\"<ul><li>Login funciona</li><li>Erro aparece direito</li><li>Nada quebra — de preferência</li></ul>\"\r\n  }, \r\n  {\r\n    \"op\" : \"add\",\r\n    \"path\" :\"/fields/Microsoft.VSTS.Common.Priority\",\r\n    \"value\":\"3\"\r\n  },\r\n  {\r\n    \"op\" : \"add\",\r\n    \"path\" :\"/fields/System.Tags\",\r\n    \"value\":\"auth; backend; critical\"\r\n  },\r\n  {\r\n    \"op\" : \"add\",\r\n    \"path\" :\"/fields/Microsoft.VSTS.Common.Risk\",\r\n    \"value\":\"2 - Medium\"\r\n  }\r\n]",
